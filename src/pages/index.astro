---
import BaseLayout from '../layouts/BaseLayout.astro';
import Navbar from '../components/Navbar.astro';
import Hero from '../components/Hero.astro';
import Servicios from '../components/Servicios.astro';
import MetodologiaPreview from '../components/MetodologiaPreview.astro';
import ExperienciaPreview from '../components/ExperienciaPreview.astro';
import Testimonios from '../components/Testimonios.astro';
import CTAWhatsApp from '../components/CTAWhatsApp.astro';
import Contacto from '../components/Contacto.astro';
import Footer from '../components/Footer.astro';
---

<BaseLayout>
  <Navbar />
  <main class="pt-24">
    <Hero />
    <Servicios />
    <MetodologiaPreview />
    <ExperienciaPreview />
    <section id="testimonios">
      <Testimonios />
    </section>
    <CTAWhatsApp />
    <Contacto />
  </main>
  <Footer />
</BaseLayout>

